{
  "name": "@nest-demo/backend",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "author": "",
  "license": "UNLICENSED",
  "scripts": {
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "repl": "pnpm run start -- --entryFile repl",
    "compodoc": "npx compodoc -p tsconfig.json -s -o",
    "madge": "madge --circular --dot --extensions ts src/ > graph.gv",
    "dev": "pnpm run start",
    "dev:watch": "pnpm run start:watch",
    "dev:debug": "pnpm run start:debug",
    "build": "cross-env NODE_ENV=production nest build",
    "start": "cross-env NODE_ENV=development nest start",
    "start:watch": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "typeorm": "NODE_ENV=development typeorm-ts-node-esm -d ./src/config/database.config.js",
    "migration:create": "pnpm run typeorm migration:create ./src/database/migrations/initData",
    "migration:generate": "pnpm run typeorm migration:generate ./src/database/migrations/update-table_$(echo $npm_package_version | sed 's/\\./_/g')",
    "migration:run": "pnpm run typeorm -- migration:run",
    "migration:revert": "pnpm run typeorm -- migration:revert",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@keyv/redis": "^3.0.1",
    "@liaoliaots/nestjs-redis": "^10.0.0",
    "@nestjs/axios": "^3.1.1",
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^10.4.7",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.7",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.6",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.7",
    "@nestjs/platform-socket.io": "^10.4.7",
    "@nestjs/swagger": "^8.0.1",
    "@nestjs/typeorm": "^10.0.2",
    "@socket.io/redis-adapter": "^8.3.0",
    "@socket.io/redis-emitter": "^5.1.0",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.7.6",
    "cache-manager-ioredis-yet": "^2.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "keyv": "^5.1.3",
    "libphonenumber-js": "^1.11.12",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "mysql2": "^3.11.3",
    "nanoid": "^3.3.7",
    "nest-winston": "^1.9.7",
    "nestjs-cls": "^4.4.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "qs": "^6.13.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.20",
    "uuid": "^11.0.2",
    "winston": "^3.16.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.26",
    "@nestjs/cli": "^10.4.7",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.7",
    "@types/cache-manager": "^4.0.6",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "cross-env": "^7.0.3",
    "graphviz": "^0.0.9",
    "jest": "^29.7.0",
    "madge": "^8.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
